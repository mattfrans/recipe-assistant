            "ingredients": [
                "1.5 cups arborio rice",
                "500g mixed mushrooms",
                "1 onion",
                "3 cloves garlic",
                "1/2 cup white wine",
                "6 cups vegetable stock",
                "1/2 cup parmesan",
                "2 tablespoons butter",
                "fresh thyme",
                "olive oil"
            ],
            "instructions": [
                "Heat stock in a separate pot",
                "Sauté mushrooms until golden",
                "Cook onion and garlic until soft",
                "Add rice and toast lightly",
                "Add wine and simmer until absorbed",
                "Add hot stock one ladle at a time",
                "Stir continuously until rice is creamy",
                "Fold in mushrooms and parmesan",
                "Finish with butter and thyme"
