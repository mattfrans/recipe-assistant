        {
            "name": "Classic French Onion Soup",
            "cuisine": "French",
            "prep_time": "15 minutes",
            "cook_time": "1 hour",
            "servings": 6,
            "ingredients": [
                "6 large onions",
                "4 tablespoons butter",
                "2 tablespoons olive oil",
                "1 teaspoon sugar",
                "2 cloves garlic",
                "8 cups beef broth",
                "1/2 cup red wine",
                "1 bay leaf",
                "4 sprigs fresh thyme",
                "baguette slices",
                "gruyere cheese"
            ],
            "instructions": [
                "Slice onions thinly",
                "Cook onions in butter and oil until caramelized (45 minutes)",
                "Add sugar to help caramelization",
                "Add minced garlic and cook briefly",
                "Pour in wine and scrape up brown bits",
                "Add broth, bay leaf, and thyme",
                "Simmer for 30 minutes",
                "Toast baguette slices",
                "Top soup with bread and cheese",
                "Broil until cheese is bubbly"
